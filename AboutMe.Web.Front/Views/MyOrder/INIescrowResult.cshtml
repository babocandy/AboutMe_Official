@model AboutMe.Domain.Entity.Order.INIESCROW_CONFIRM
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>

    <title>INIescrow</title>
    <meta http-equiv="Content-Type" content="text/html; charset=euc-kr">



    <style type="text/css">
        BODY {
            font-size: 9pt;
            line-height: 160%;
        }

        TD {
            font-size: 9pt;
            line-height: 160%;
        }

        INPUT {
            font-size: 9pt;
        }

        .emp {
            background-color: #E0EFFE;
        }
    </style>

</head>

<body>
    <table border=0 width=500>
        <tr>
            <td>
                <hr noshade size=1>
                <b>구매 확인(거절) 처리 결과</b>
                <hr noshade size=1>
            </td>
        </tr>
    </table>
    <br>

    <table border=0 width=500>


        <tr>
            <td align=right nowrap>결과코드 : </td>
            <td><font class=emp>@Model.Resultcode</font></td>
        </tr>

        <tr>
            <td align=right nowrap>결과메세지 : </td>
            <td><font class=emp>@Model.ResultMsg</font></td>
        </tr>

        <tr>
            <td align=right nowrap>처리날짜(YYYYMMDD)  : </td>
            <td><font class=emp>@Model.CNF_Date</font></td>
        </tr>

        <tr>
            <td align=right nowrap>처리시각(hhmmss)  : </td>
            <td><font class=emp>@Model.CNF_Time</font></td>
        </tr>

        <tr>
            <td colspan=2><hr noshade size=1></td>
        </tr>
        <tr>
            <td align=right nowrap>취소처리날짜(YYYYMMDD)  : </td>
            <td><font class=emp>@Model.DNY_Date</font></td>
        </tr>

        <tr>
            <td align=right nowrap>취소처리시각(hhmmss)  : </td>
            <td><font class=emp>@Model.DNY_Time</font></td>
        </tr>

        <tr>
            <td colspan=2><hr noshade size=1></td>
        </tr>
        <tr>
            <td align=center colspan=2><input type="button" name="btn" value="창닫기" onclick="opener.location.reload();self.close();" /></td>
        </tr>
    </table>
</body>
</html>
