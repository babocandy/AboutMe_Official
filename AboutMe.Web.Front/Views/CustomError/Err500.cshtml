
@{
    Layout = null;
}

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <meta name="viewport" content="width=1080, target-densitydpi=medium-dpi" />
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
    <title>ABOUT ME</title>
    <style type="text/css">
        html, body {
            margin: 0;
            padding: 0;
            font-family: 'NanumBarunGothicLight';
        }

        p, h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, table, th, td, form, fieldset, legend, input, textarea, button, img {
            margin: 0;
            padding: 0;
        }

        input, button, select, textarea {
            font-family: ''NanumBarunGothicLight'','맑은 고딕','Malgun Gothic','돋움',dotum,Helvetica,sans-serif;
            vertical-align: middle;
        }

            input[type="checkbox"], input[type="radio"] {
                margin: 1px 0 0 0;
                vertical-align: top;
            }

        h1, h2, h3, h4, h5, h6 {
            font-size: 0;
        }

        ul, ol, li {
            list-style: none;
        }

        img, fieldset, iframe {
            border: 0 none;
            vertical-align: top;
        }

        table {
            border-collapse: collapse;
        }

        a {
            color: #585858;
            text-decoration: none;
        }

        caption, legend {
            display: none;
        }

        em, address {
            font-style: normal;
        }

        .clear {
            clear: both;
            width: 0;
            height: 0;
            padding: 0;
            margin: 0;
            font-size: 0;
            line-height: 0;
        }

        .hand {
            cursor: pointer;
        }

        .non {
            color: #fff;
        }

        hr, .connone {
            display: none;
        }

        div {
            -webkit-text-size-adjust: none;
        }

        p {
            word-wrap: break-word;
            word-break: break-all;
            -webkit-text-size-adjust: none;
        }

        .error_wrap {
            width: 1080px;
            margin: 0 auto;
        }

        .error_h {
        }

        .error_c {
            margin-top: 50px;
            text-align: center;
        }
    </style>
</head>
<body>
    <div class="error_wrap">
        <div class="error_h"><a href="http://www.aboutmeshop.com/"><img src="/aboutCom/images/Common/error_top.jpg" alt="" /></a></div>
        <div class="error_c">
            <a href="http://www.aboutmeshop.com/"><img src="/aboutCom/images/Common/error_505.jpg" alt="" /></a>
        </div>
    </div>
</body>
</html>
