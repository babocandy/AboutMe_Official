@model List<AboutMe.Domain.Entity.Exhibit.SP_EXHIBIT_PCODE_LINK_ALL_Result>
@if (Model.Count > 0)
{ 
    <div class="bnr_area">
    @foreach (var item in Model)
    {
        <div class="bnr"><a href="@item.LINK_URL"><img src="@item.MOBILE_BANNER" alt="@item.EXHIBIT_NAME" /></div>
    }    
    </div>
}