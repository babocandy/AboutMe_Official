@model AboutMe.Web.Mobile.Models.BaseDisplayerViewModel
@using AboutMe.Web.Mobile.Controllers
@{
    Layout = null;

    var myController = ViewContext.Controller as ChildActionDisplayerController;
}
<div class="bnr_area">

    @foreach (var item in Model.List)
    {
        <div class="bnr"><img src="@(string.Format("{0}{1}", myController._img_path_display, item.IMG))" alt="" /></div>
    }

</div>
